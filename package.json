  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint:recommended",
      "prettier"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@4.1.0"
}
